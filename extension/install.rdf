<?xml version="1.0"?>

<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
  <Description about="urn:mozilla:install-manifest">
    <em:id>qfb-pivot@mozillamessaging.com</em:id>
    <em:version>1.1</em:version>
    <em:type>2</em:type>
    <em:bootstrap>false</em:bootstrap>
    <em:unpack>true</em:unpack>

    <!-- Target Application this extension can install into, 
         with minimum and maximum supported versions. --> 

    <!-- Thunderbird -->
    <em:targetApplication>
      <Description>
        <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
        <em:minVersion>5.0</em:minVersion>
        <em:maxVersion>5.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- Front End MetaData -->
    <em:name>QFB Pivot</em:name>
    <em:description>Quick Filter Bar Pivot extension</em:description>
    <em:creator>Mozilla</em:creator>
    <em:iconURL></em:iconURL>
    <em:homepageURL></em:homepageURL>
    <em:optionsURL></em:optionsURL>
    <em:updateURL></em:updateURL>
  </Description>
</RDF>
